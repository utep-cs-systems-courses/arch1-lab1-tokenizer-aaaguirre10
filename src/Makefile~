!# Compiler Flags. -g = debug -03 = Optimazation
CFLAGS = -g -O3

# Default target
all: uimain

# File dependencies
tokenizer.o: tokenizer.h
history.o: history.h
uimain.o: tokenizer.h

# $@=target, $^= prereq
uimain: tokenizer.o history.o uimain.o
	cc -o $@ $^ $(CFLAGS)

# Remove all files generated by compilation.
clean:
	rm -f *.o *.# *.~ uimain

# Run code
run: uimain
	./uimain
